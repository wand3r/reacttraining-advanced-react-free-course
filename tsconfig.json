{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": true,
    "removeComments": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es2016"]
  }
}
